{
  "name": "mj-script",
  "version": "1.4.8",
  "description": "企业级脚手架",
  "main": "index.js",
  "scripts": {
    "services-dev": "nodemon ./services/index.js",
    "services": "pm2 start ./services/index.js --name mjScriptServices",
    "services-stop": "pm2 stop mjScriptServices",
    "services-restart": "pm2 restart mjScriptServices",
    "services-del": "pm2 delete mjScriptServices",
    "services-list": "pm2 list",
    "build": "rollup -c"
  },
  "bin": {
    "mj-script": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HuskyToMa/mj_script.git"
  },
  "keywords": [
    "script",
    "构建",
    "build",
    "webpack"
  ],
  "author": "zhanan",
  "license": "ISC",
  "dependencies": {
    "archiver": "4.0.1",
    "argv_store": "^1.0.6",
    "axios": "0.19.2",
    "chalk": "3.0.0",
    "decompress": "4.2.1",
    "fs-extra": "9.0.0",
    "inquirer": "7.1.0",
    "koa": "2.12.1",
    "koa-router": "9.0.1",
    "mj-config": "^1.0.0",
    "ora": "4.0.3",
    "pm2": "4.4.0",
    "request": "2.88.2",
    "rimraf": "3.0.2",
    "tapable": "1.1.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-external-helpers": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "nodemon": "^2.0.4",
    "rollup": "^2.22.1",
    "rollup-plugin-babel": "^4.4.0"
  }
}
